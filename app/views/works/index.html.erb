
<h1>Tags</h1>

<div id='tag_cloud'>
  <% tag_cloud Work.tag_counts, %[s m l] do |tag, css_class|   %>
    <%= link_to tag.name, tag_path(tag.name), class: css_class %>
  <% end %>

</div>

<h1>Works</h1>
<% @works.each do |work| %>

  <%= work.title %>
  <br>
<% end %>

<h1>Artists</h1>
<% @artists.each do |artist| %>

  <%= artist.username %>
  <br>
<% end %>

<h1>Users</h1>
<% @users.each do |user| %>

  <%= user.username %>
  <br>
<% end %>