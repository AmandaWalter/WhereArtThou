<div class="search-page-background">
	<div class="container">

		<h2>works</h2>
		<% @works.each do |work| %>
			<div><%= work.title %> </div>
		<% end %>
		<p>_______________________________</p>

		<h2>artists</h2>
		<% @artists.each do |artist| %>
			<div><%= artist.username.camelize %> </div>
		<% end %>

		<p>_______________________________</p>

		<h2>venues</h2>
		<% @venues.each do |venue| %>
			<div><strong><%= venue.name.camelize %></strong>, <%= venue.city.camelize %> </div>
		<% end %>

		<p>_______________________________</p>

		<h2>events</h2>
		<% @events.each do |event| %>
			<div><strong><%= event.name.camelize %></strong>, <%= event.city.camelize unless event.city.nil? %> </div>
		<% end %>

		<p>_______________________________</p>

		<h2>users</h2>
		<% @users.each do |user| %>
			<div><%= user.username.camelize %> </div>
		<% end %>






		
	</div>
</div>