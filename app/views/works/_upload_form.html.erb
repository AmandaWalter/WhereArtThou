<%= form_for([@artist, @work], :html => { :multipart => true }) do |f| %>
  <div class="form-group">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>

  <div class="form-group">
    <%= f.label :year_created %><br />
    <%= f.text_field :year %>
  </div>

  <p>
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </p>

  <p>
    <%= f.label :medium %><br />
    <%= f.text_field :medium %>
  </p>

  <p>
    <%= f.label :style %><br />
    <%= f.text_field :style %>
  </p>

  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.file_field :image %>
  </p>

  <p>
    <%= f.submit "Submit" %>
  </p>
  <% end%>
