<%= form_for([@artist, @work], url: artist_works_path(@artist), :html => { :multipart => true }) do |f| %>
  <% if @work.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@work.errors.count, "error") %> prohibited this image from being saved:</h2>
      <ul>
      <% @work.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>

  <div class="form-group">
    <%= f.label :year_created %><br />
    <%= f.text_field :year %>
  </div>

  <p>
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </p>

  <p>
    <%= f.label :medium %><br />
    <%= f.text_field :medium %>
  </p>

  <p>
    <%= f.label :style %><br />
    <%= f.text_field :style %>
  </p>

  <p>
    <%= f.label :tag_list, 'Tags (seperated by commas)' %><br />
    <%= f.text_field :tag_list %>
  </p>

  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.file_field :image %>
  </p>

  <p>
    <%= f.submit "Submit" %>
  </p>
<% end%>
